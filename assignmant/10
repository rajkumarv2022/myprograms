#include <stdio.h>
int main()
{
 int a,b;
 printf("Enter the ammount :\n");
 scanf("%d",&a);
 if(a<=50000)
 printf("The intrest is : %d",(a*1)/100);
 else if(a>50000&&a<=75000)
 printf("The intrest is : %d",(a*2)/100);
 else if(a>75000&&a<=100000)
 printf("The intrest is : %d",(a*3)/100);
 else if(a>100000&&a<=250000)
 printf("The intrest is : %d",(a*4)/100);
 else if(a>250000)
 printf("The intrest is : %d",(a*5)/100);
 else
 printf("Enter valid amount");
 return 0;
}
